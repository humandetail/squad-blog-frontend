    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.11",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
