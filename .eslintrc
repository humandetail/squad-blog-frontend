{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:nuxt/recommended"
  ],
  "plugins": [],
  "rules": {
    "vue/no-multiple-template-root": 0,
    "vue/multi-word-component-names": 0,
    "vue/no-v-model-argument": 0,
    "vue/no-v-html": 0,
    "vue/no-v-for-template-key": 0,
    "vue/valid-v-slot": 0
  }
}
